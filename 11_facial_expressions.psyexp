        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="import * as uiautomator2 from 'uiautomator2';&amp;#10;import {Thread} from 'threading';&amp;#10;if (EMGTLV_enabled) {&amp;#10;    deviceSerial = settingsInfo[&quot;TLV-device&quot;];&amp;#10;    devices = [new uiautomator2.Device(deviceSerial)];&amp;#10;    add_button_id = &quot;com.xtrodes.xtrodesapp:id/btn_add&quot;;&amp;#10;    function addFirstEventContainer(device) {&amp;#10;        var new_event;&amp;#10;        new_event = device({&quot;resourceId&quot;: add_button_id})[0];&amp;#10;        new_event.click();&amp;#10;    }&amp;#10;    function returnToEventScreen(device) {&amp;#10;        var eventlog_btn, sideBar_dashboard_btn;&amp;#10;        sideBar_dashboard_btn = device({&quot;resourceId&quot;: &quot;com.xtrodes.xtrodesapp:id/container&quot;})[0];&amp;#10;        sideBar_dashboard_btn.click();&amp;#10;        eventlog_btn = device({&quot;resourceId&quot;: &quot;com.xtrodes.xtrodesapp:id/btn_event_log&quot;})[0];&amp;#10;        eventlog_btn.click();&amp;#10;    }&amp;#10;    function PrepareEventTextAndButton(device, text) {&amp;#10;        var add_buttons, event_text, send_event;&amp;#10;        add_buttons = device({&quot;resourceId&quot;: add_button_id});&amp;#10;        if ((add_buttons.count === 0)) {&amp;#10;            returnToEventScreen(device);&amp;#10;            add_buttons = device({&quot;resourceId&quot;: add_button_id});&amp;#10;        }&amp;#10;        if ((add_buttons.count === 1)) {&amp;#10;            addFirstEventContainer(device);&amp;#10;            add_buttons = device({&quot;resourceId&quot;: add_button_id});&amp;#10;        }&amp;#10;        send_event = add_buttons[1];&amp;#10;        event_text = device({&quot;resourceId&quot;: &quot;com.xtrodes.xtrodesapp:id/et_name&quot;})[0];&amp;#10;        event_text.set_text(text);&amp;#10;        return send_event;&amp;#10;    }&amp;#10;    function ButtonClickThread(button) {&amp;#10;        button.click();&amp;#10;    }&amp;#10;    function sendTriggerInBothDevices(text) {&amp;#10;        var buttons;&amp;#10;        buttons = function () {&amp;#10;    var _pj_a = [], _pj_b = devices;&amp;#10;    for (var _pj_c = 0, _pj_d = _pj_b.length; (_pj_c &lt; _pj_d); _pj_c += 1) {&amp;#10;        var device = _pj_b[_pj_c];&amp;#10;        _pj_a.push(new PrepareEventTextAndButton(device, text));&amp;#10;    }&amp;#10;    return _pj_a;&amp;#10;}&amp;#10;.call(this);&amp;#10;        function () {&amp;#10;    var _pj_a = [], _pj_b = buttons;&amp;#10;    for (var _pj_c = 0, _pj_d = _pj_b.length; (_pj_c &lt; _pj_d); _pj_c += 1) {&amp;#10;        var x = _pj_b[_pj_c];&amp;#10;        _pj_a.push(new Thread({&quot;target&quot;: ButtonClickThread, &quot;args&quot;: [x]}).start());&amp;#10;    }&amp;#10;    return _pj_a;&amp;#10;}&amp;#10;.call(this);&amp;#10;    }&amp;#10;    sendTriggerInBothDevices(&quot;Init Sequence&quot;);&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
